{
  "name": "bitportal",
  "version": "1.5.1",
  "description": "bitportal client",
  "main": "main.js",
  "engines": {
    "node": ">=8.0.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "build": "rimraf static && npm run build:server && npm run build:client",
    "build:dev": "rimraf static && cross-env NODE_ENV=development APP_ENV=development TARGET=web webpack --progress --colors",
    "build:staging": "rimraf static && npm run build:server:staging && npm run build:client:staging",
    "build:test": "rimraf static && npm run build:server:test && npm run build:client:test",
    "build:pre": "rimraf static && npm run build:server:pre && npm run build:client:pre",
    "build:demo": "rimraf static && npm run build:server:demo && npm run build:client:demo",
    "build:client": "cross-env NODE_ENV=production APP_ENV=production TARGET=web webpack --progress --colors",
    "build:server": "cross-env NODE_ENV=production APP_ENV=production TARGET=node webpack --progress --colors",
    "build:client:staging": "cross-env NODE_ENV=production APP_ENV=staging TARGET=web webpack --progress --colors",
    "build:server:staging": "cross-env NODE_ENV=production APP_ENV=staging TARGET=node webpack --progress --colors",
    "build:client:test": "cross-env NODE_ENV=production APP_ENV=test TARGET=web webpack --progress --colors",
    "build:server:test": "cross-env NODE_ENV=production APP_ENV=test TARGET=node webpack --progress --colors",
    "build:client:pre": "cross-env NODE_ENV=production APP_ENV=pre TARGET=web webpack --progress --colors",
    "build:server:pre": "cross-env NODE_ENV=production APP_ENV=pre TARGET=node webpack --progress --colors",
    "client": "cross-env NODE_ENV=production http-server static",
    "server": "cross-env NODE_ENV=production nodemon --icu-data-dir=/usr/local/lib/node_modules/full-icu static/app.js",
    "start": "cross-env NODE_ENV=development APP_ENV=development TARGET=web webpack-dev-server -d --history-api-fallback --inline --progress --port 3009 --host dev.bitportal.io",
    "test": "NODE_ENV=test mocha --icu-data-dir=/usr/local/lib/node_modules/full-icu --require babel-register --require babel-polyfill --require test/helpers/setup.js --recursive",
    "cover": "nyc --reporter=lcov --reporter=text --reporter=html --require babel-register --extension .js npm test",
    "lint": "eslint browser server shared test",
    "mobile": "watchman watch-del-all && rm -fr $TMPDIR/react-* && cross-env NODE_ENV=production node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "mobile:test": "rm -fr $TMPDIR/react-* && cross-env NODE_ENV=production APP_ENV=test node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "ios": "react-native run-ios --scheme bitportal-development",
    "ios:staging": "react-native run-ios --scheme bitportal-staging",
    "ios:production": "react-native run-ios --scheme bitportal-production",
    "android": "ENVFILE=.env.development react-native run-android",
    "android:staging": "ENVFILE=.env.staging react-native run-android",
    "android:production": "ENVFILE=.env.production react-native run-android",
    "archive:android": "cd android && ./gradlew assembleRelease",
    "bundle:ios": "react-native bundle --entry-file mobile/index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:desktop": "rimraf bundle && cross-env NODE_ENV=production APP_ENV=development TARGET=\"electron-renderer\" webpack --progress --colors",
    "desktop": "electron bundle/index.html",
    "tsc": "tsc --watch"
  },
